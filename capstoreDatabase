  create table product(
    prod_Id int primary key auto_increment, 
    prod_Name varchar(50), 
    prod_Price double(10,2), 
    prod_Quantity int, 
    prod_Discount double(6,2), 
    prod_Category varchar(50), 
    prod_Desc varchar(500), 
    prod_Image varchar(500), 
    merchant_Id int
  );
